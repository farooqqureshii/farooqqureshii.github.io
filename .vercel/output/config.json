{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/now-playing/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/spotify-now-playing/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/spotify-now-playing(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/api(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/artifacts/?$",
			"dest": "_render"
		},
		{
			"src": "^/graph-view/?$",
			"dest": "_render"
		},
		{
			"src": "^/misc/books/?$",
			"dest": "_render"
		},
		{
			"src": "^/misc/music/?$",
			"dest": "_render"
		},
		{
			"src": "^/misc/?$",
			"dest": "_render"
		},
		{
			"src": "^/projects/?$",
			"dest": "_render"
		},
		{
			"src": "^/robots\\.txt/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/writing/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}